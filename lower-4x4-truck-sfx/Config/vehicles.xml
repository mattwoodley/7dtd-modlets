<?xml version="1.0" encoding="UTF-8" ?>

<configs>

    <!-- xpath is selected by each gear's 'volume min' and 'volume max' attributes -->
    <!-- gear 1 also selects vehicleJokeblimp -->

    <!-- Gear 1 -->
    <set xpath="//property[contains(@value, '.7, .7')]/@value">
        500,2500,-1400,800,0,700,2200,900,
        Vehicles/Suv/suv_accel1,
        Vehicles/Suv/suv_decel1,
        0,.7,1,.1,-9,.12,.1,Vehicles/Suv/suv_idle_lp,
        -.4,-.02,.3,.3,-.2,9,.2,Vehicles/Suv/suv_max_speed_lp
    </set>
    
    <!-- Gear 2 -->
    <set xpath="//property[contains(@value,'.2, .4')]/@value">
        500,2500, -2000,1000,2000, 600,2200,800,
        Vehicles/Suv/suv_accel2,
        Vehicles/Suv/suv_decel1,
        -.25, -.05, .1, .2, -1, 1, .1, Vehicles/Suv/suv_max_speed_lp
    </set>
    
    <!-- Gear 3 -->
    <set xpath="//property[contains(@value,'.3, .6')]/@value">
        500,2500, -2000,1000,2000, 500,2200,1000,
        Vehicles/Suv/suv_accel3,
        Vehicles/Suv/suv_decel2,
        -.28, -.07, .1, .2, -1, 1, .1, Vehicles/Suv/suv_max_speed_lp
    </set>

    <!-- Gear 4 -->
    <set xpath="//property[contains(@value,'.6, .9')]/@value">
        500,2500, -2000,1000,2000, 500,9999,9999,
        ,
        Vehicles/Suv/suv_decel2,
        -.31, -.08, .2, .4, -1, 1, .1, Vehicles/Suv/suv_max_speed_lp
    </set>
</configs>
